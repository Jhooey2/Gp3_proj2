<div class="bids">
  {{#each this}}
    <div class="card">
      <h3 class="card-header"></h3>
      <section class="bid card-body">
        <div class="text">
          {{quote}}
        </div>
        <div class="meta">
          -{{user.username}},
          {{format_date created_at}}
        </div>
      </section>
    </div>
  {{/each}}
</div>