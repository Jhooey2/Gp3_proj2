<div class="ratings">
  {{#each this}}
    <div class="card">
      <h3 class="card-header"></h3>
      <section class="rating card-body">
        <div class="text">
          {{rating_text}}
        </div>
        <div class="text">
            {{rating_id}}
        </div>
        <div class="meta">
          -{{user.username}},
          {{format_date created_at}}
        </div>
      </section>
    </div>
  {{/each}}
</div>